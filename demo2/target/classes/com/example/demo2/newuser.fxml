<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="shadow" stylesheets="@style1.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.logincontroller">
   <children>
      <VBox layoutX="18.0" layoutY="24.0" prefHeight="61.0" prefWidth="565.0" styleClass="shadow" stylesheets="@style1.css">
         <children>
            <Label prefHeight="41.0" prefWidth="557.0" styleClass="container" text="                                Welcome User!!!">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="136.0" layoutY="93.0" prefHeight="215.0" prefWidth="351.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="42.0" prefWidth="326.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="117.0" stylesheets="@style1.css" text=" NAME :">
                     <font>
                        <Font name="Arial Black" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_name" prefHeight="26.0" prefWidth="222.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="351.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="115.0" stylesheets="@style1.css" text="MOBILE NO. :">
                     <font>
                        <Font name="Arial Black" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_mobile" prefHeight="26.0" prefWidth="224.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="351.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="130.0" stylesheets="@style1.css" text=" EMAIL :">
                     <font>
                        <Font name="Arial Black" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_email" prefHeight="26.0" prefWidth="237.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="351.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="117.0" stylesheets="@style1.css" text="USERNAME :">
                     <font>
                        <Font name="Arial Black" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_user" prefHeight="26.0" prefWidth="194.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="41.0" prefWidth="351.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="120.0" stylesheets="@style1.css" text=" PASSWORD :">
                     <font>
                        <Font name="Arial Black" size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tf_pass" prefHeight="26.0" prefWidth="194.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="BOTTOM_RIGHT" layoutX="125.0" layoutY="308.0" prefHeight="48.0" prefWidth="351.0" styleClass="shadow" stylesheets="@style1.css">
         <children>
            <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="165.0" styleClass="container" stylesheets="@style1.css" text="Back">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
            <HBox prefHeight="48.0" prefWidth="22.0" />
            <Button layoutX="164.0" layoutY="26.0" mnemonicParsing="false" onAction="#onSignUpButtonClick" prefHeight="32.0" prefWidth="161.0" styleClass="container" stylesheets="@style1.css" text="Create Account">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
